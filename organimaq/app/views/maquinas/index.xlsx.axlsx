
wb = xlsx_package.workbook
wb.add_worksheet(name: "Report") do |sheet|
  negrita = sheet.styles.add_style :bold => true
  sheet.add_row ["Codigo", "Maquina", "Tipo", "Estado","Patente","Chasis","Marca", "AÃ±o", "Horometro Actual", "Kilometraje Actual", "Valor Hora", "Valor Dia"], :height =>35, :style => [negrita]  ## Titulos
 
  @maquinas.each do |mq|
    sheet.add_row [mq.codigo, mq.nombre, TipoMaquina.find(mq.tipo_maquina_id).nombre,EstadoMaquina.find(mq.estado_maquina_id).nombre , mq.patente, mq.chasis, mq.marca, mq.anio, mq.horometro_actual, mq.km_actual, mq.valor_hora, mq.valor_dia ]
  end
end